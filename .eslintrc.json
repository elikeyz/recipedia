{
    "extends": [
      "airbnb",
      "plugin:react/recommended",
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "commonjs": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "parser": "babel-eslint"
}
